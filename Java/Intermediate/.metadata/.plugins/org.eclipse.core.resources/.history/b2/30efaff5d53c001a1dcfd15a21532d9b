
public class MaxMain {
public static void main(String[] args) {
	/*
	 * (num1,num2) : No of arguments to the method
	 *  -> : lambda operator
	 *  right side : method defination
	 */
	Max ref = (num1,num2)->(num1>num2)?num1:num2;
	int maxNumber = ref.max(12, 9);
	System.out.println("maximum = "+maxNumber);
}
}
