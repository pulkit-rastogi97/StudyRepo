package question1;

public class Stack {

	int arr[] = new int[50];
	int top;
	public Stack()
	{
		top = -1;
	}
	
	public void push(int i)
	{
		if(top==50)
		{
			System.out.println("Stack OverFlow");
			return;
		}
		top++;
		arr[top]=i;
	}
	
	public int pop()
	{
		if(top==-1)
		{
			System.out.println("Stack Underflow");
			//return Integer.TYPE;
		}
		return -1;
	}
}
