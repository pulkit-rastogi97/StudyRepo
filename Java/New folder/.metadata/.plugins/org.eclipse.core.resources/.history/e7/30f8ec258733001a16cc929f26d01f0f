package Exception;

public class MainClassAccount {

	public static void main(String[] args) {
		Account obj = new Account("Pooja",5000);
		System.out.println(obj.getBalance("Pooja"));
		
		obj.withdraw(3000);
		System.out.println("balance = "+obj.getBalance(obj.getName()));
		obj.withdraw(3000);
	}
}
